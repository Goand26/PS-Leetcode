/*https://leetcode.com/problems/count-odd-numbers-in-an-interval-range/description/?envType=study-plan&id=programming-skills-i/*


class Solution {
    		public int countOdds(int low, int high) {

			  int diff=high-low;
			  int count=diff/2;

				if(low%2!=0 || high%2!=0)
					count++;

				return count;
			}

}
